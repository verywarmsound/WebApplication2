<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("eGOV.com", "Index", "Home", new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "Index", "Home", protocol: null, hostName: null, fragment: "mission", routeValues: null, htmlAttributes: null)</li>
                    <li>@Html.ActionLink("Services", "Index", "Home", protocol: null, hostName: null, fragment: "services", routeValues: null, htmlAttributes: null)</li>
                    <li>@Html.ActionLink("Contact", "Index", "Home", protocol: null, hostName: null, fragment: "contact", routeValues: null, htmlAttributes: null)</li>
