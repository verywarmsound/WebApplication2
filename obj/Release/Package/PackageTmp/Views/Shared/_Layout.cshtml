            </div>
        </div>
    </div>
    @Html.Partial("_Carousel")
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - ASP.NET Application by Olga Korpacheva</p>
            <p>Photos are from www.pexels.com, authors: Scott Webb, Leah Kelley, Snapwire</p>
        </footer>
    </div>
    <div class="container body-content">
        <div class="page" id="contact">
            <h3>With any questions please contact me:</h3>
            <address>
                Olga Korpacheva<br />
